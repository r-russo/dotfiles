start-pulseaudio-x11 &
numlockx on &
setxkbmap dvorak es &
(compton -b --config ~/.config/compton.conf && nvidia-settings --load-config-only) &
mpd &

#volwheel &
(sleep 2 && tint2) &
(sleep 2 && conky -c ~/.conky/conkyrc) &
(sleep 2 && conky -c ~/.conky/conkytodo) &
#feh --bg-fill ~/Im√°genes/Fondos/$(cat ~/.fondo) &
nitrogen --restore &
sh ~/.dzen/main.sh &
/usr/lib/deja-dup/deja-dup/deja-dup-monitor &
xfce4-power-manager &

(sleep 4 && thunderbird) &
(sleep 1 && liferea) &
(sleep 3 && deluge) &
(sleep 2 && sonata --hidden) &
(sleep 2 && dropboxd) &
